kind: Route
apiVersion: route.openshift.io/v1
metadata:
  annotations:
    console.alpha.openshift.io/overview-app-route: "false"
  name: "{{ _nexus_name }}-registry"
  namespace: "{{ _nexus_namespace }}"
  labels:
    app: "{{ _nexus_name }}"
spec:
  to:
    kind: Service
    name: "{{ _nexus_name }}-registry"
  tls:
    insecureEdgeTerminationPolicy: Redirect
    termination: edge
